{
  "name": "vTextarea",
  "props": {
    "name": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "modTextarea": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "modLabel": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "base": {
      "type": [
        "String"
      ],
      "default": "textarea",
      "globalDefault": true
    },
    "variant": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "validateOn": {
      "type": [
        "String"
      ],
      "default": "blur"
    },
    "validateMode": {
      "type": [
        "String"
      ],
      "default": "silent"
    },
    "rules": {
      "type": [
        "Object"
      ],
      "default": []
