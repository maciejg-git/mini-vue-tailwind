{
  "name": "vSelect",
  "props": {
    "name": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "base": {
      "type": [
        "String"
      ],
      "default": "select"
    },
    "variant": {
      "type": [
        "String",
        "Boolean"
      ],
      "default": ""
    },
    "modelValue": {
      "type": [
        "String",
        "Object",
        "Boolean",
        "Number"
      ],
      "default": "undefined"
    },
    "useLoader": {
      "type": [
        "Boolean"
      ],
      "default": true
    },
    "icon": {
      "type": [
        "String",
        "Object"
      ],
      "default": ""
    },
    "clearable": {
      "type": [
        "Boolean"
      ],
      "default": "undefined"
    },
    "validationState": {
      "type": [
        "String"
      ],
      "default": null
    },
    "items": {
      "type": [
        "Array"
      ],
      "default": []
    },
    "multiple": {
      "type": [
        "Boolean"
      ],
      "default": false
    },
    "maxMultiple": {
      "type": [
        "Number"
      ],
      "default": 9999
    },
    "autocomplete": {
      "type": [
        "Boolean"
      ],
      "default": false
    },
    "itemText": {
      "type": [
        "String"
      ],
      "default": "text"
    },
    "itemValue": {
      "type": [
        "String"
      ],
      "default": "value"
    },
    "filterKeys": {
      "type": [
        "Array"
      ],
      "default": []
    },
    "noFilter": {
      "type": [
        "Boolean"
      ],
      "default": false
    },
    "noPagination": {
      "type": [
        "Boolean"
      ],
      "default": false
    },
    "emptyDataMessage": {
      "type": [
        "String"
      ],
      "default": "No data available"
    },
    "input": {
      "type": [
        "Object"
      ],
      "default": {}
    },
    "chevron": {
      "type": [
        "Object"
      ],
      "default": {}
    },
    "card": {
      "type": [
        "Object"
      ],
      "default": {}
    },
    "itemsPerPage": {
      "type": [
        "Number"
      ],
      "default": 10
    },
    "transition": {
      "type": [
        "String"
      ],
      "default": "fade-scale"
    },
    "styleItem": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "placement": {
      "type": [
        "String"
      ],
      "default": "bottom-start"
    },
    "offsetX": {
      "type": [
        "Number"
      ],
      "default": 0
    },
    "offsetY": {
      "type": [
        "Number"
      ],
      "default": 0
    },
    "flip": {
      "type": [
        "Boolean"
      ],
      "default": true
    },
    "autoPlacement": {
      "type": [
        "Boolean"
      ],
      "default": false
    }
  },
  "emits": [
    "update:modelValue",
    "update:page",
    "input:value",
    "state:opened",
    "state:closed"
  ],
  "slots": [
    "selected-item",
    "max-multiple",
    "menu-item"
  ]
}