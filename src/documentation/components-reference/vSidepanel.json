{
  "name": "vSidepanel",
  "props": {
    "name": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "styleSidepanel": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "base": {
      "type": [
        "String"
      ],
      "default": "sidepanel",
      "globalDefault": true
    },
    "variant": {
      "type": [
        "String",
        "Boolean"
      ],
      "default": ""
    },
    "modelValue": {
      "type": [
        "Boolean"
      ],
      "default": false
    },
    "showCloseButton": {
      "type": [
        "Boolean"
      ],
      "default": true,
      "globalDefault": true
    },
    "sidebarLeft": {
      "type": [
        "Boolean"
      ],
      "default": false
    },
    "width": {
      "type": [
        "String"
      ],
      "default": "320px",
      "globalDefault": true
    },
    "modal": {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true
    },
    "noHeader": {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true
    },
    "closeButton": {
      "type": [
        "Object"
      ],
      "default": {},
      "globalDefault": true
    },
    "transition": {
      "type": [
        "String"
      ],
      "default": "fade-slide",
      "globalDefault": true
    }
  },
  "emits": [
    "update:modelValue"
  ],
  "slots": [
    "header",
    "default"
  ]
}