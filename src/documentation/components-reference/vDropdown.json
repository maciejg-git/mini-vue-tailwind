{
  "name": "vDropdown",
  "props": {
    "name": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "modItem": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "modHeader": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "base": {
      "type": [
        "String"
      ],
      "default": "dropdown",
      "globalDefault": true
    },
    "variant": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "placement": {
      "type": [
        "String"
      ],
      "default": "bottom-start",
      "globalDefault": true
    },
    "offsetX": {
      "type": [
        "Number"
      ],
      "default": 0,
      "globalDefault": true
    },
    "offsetY": {
      "type": [
        "Number"
      ],
      "default": 0,
      "globalDefault": true
    },
    "flip": {
      "type": [
        "Boolean"
      ],
      "default": true,
      "globalDefault": true
    },
    "autoPlacement": {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true
    },
    "modelValue": {
      "type": [
        "Boolean"
      ],
      "default": "undefined"
    },
    "autoCloseMenu": {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true
    },
    "trigger": {
      "type": [
        "String"
      ],
      "default": "click"
    },
    "transition": {
      "type": [
        "String"
      ],
      "default": "fade-scale",
      "globalDefault": true
    }
  },
  "emits": [
    "state:opened",
    "state:closed",
    "update:modelValue"
  ],
  "slots": [
    "reference",
    "default"
  ]
}