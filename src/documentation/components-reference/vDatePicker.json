{
  "name": "vDatePicker",
  "props": {
    "name": {
      "type": [
        "String"
      ],
      "default": ""
    },
    "styleDatepicker": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "styleWeekdayBar": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "styleWeekday": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "styleButton": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "styleDay": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "styleAdjacentMonthDay": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "styleFooter": {
      "type": [
        "String"
      ],
      "default": "",
      "globalDefault": true
    },
    "base": {
      "type": [
        "String"
      ],
      "default": "datepicker",
      "globalDefault": true
    },
    "variant": {
      "type": [
        "String",
        "Boolean"
      ],
      "default": ""
    },
    "modelValue": {
      "type": [
        "String",
        "Array"
      ],
      "default": "undefined"
    },
    "locale": {
      "type": [
        "String"
      ],
      "default": "en-GB",
      "globalDefault": true
    },
    "mondayFirstWeekday": {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true
    },
    "range": {
      "type": [
        "Boolean"
      ],
      "default": false
    },
    "format": {
      "type": [
        "Object"
      ],
      "default": {
        "weekday": "long",
        "year": "numeric",
        "month": "long",
        "day": "numeric"
      },
      "globalDefault": true
    },
    "width": {
      "type": [
        "String"
      ],
      "default": "undefined"
    },
    "adjacentMonths": {
      "type": [
        "Boolean"
      ],
      "default": true,
      "globalDefault": true
    },
    "rangeHoverHighlight": {
      "type": [
        "Boolean"
      ],
      "default": true
    },
    "buttons": {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true
    },
    "secondaryButtonLabel": {
      "type": [
        "String"
      ],
      "default": "Cancel",
      "globalDefault": true
    },
    "primaryButtonLabel": {
      "type": [
        "String"
      ],
      "default": "OK",
      "globalDefault": true
    },
    "primaryButton": {
      "type": [
        "Object"
      ],
      "default": {
        "styleButton": "primary small"
      },
      "globalDefault": true
    },
    "secondaryButton": {
      "type": [
        "Object"
      ],
      "default": {
        "styleButton": "secondary small"
      },
      "globalDefault": true
    },
    "transition": {
      "type": [
        "String"
      ],
      "default": "fade"
    }
  },
  "emits": [
    "update:modelValue",
    "update:formatted",
    "state:done",
    "input:cancel"
  ],
  "slots": [
    "day"
  ]
