    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "single-line-message",
      "description": "Limits the number of displayed errors to one at time. Order of messages is the same as order of `rules` in rules prop"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "prop": "spinner",
      "description": "Attributes of input v-spinner component (loader)"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "style-close-button-wrapper",
      "description": ""
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "style-icon",
      "description": "Icon element if icon prop is true"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "style-input",
      "description": "Main input element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "style-label",
      "description": "Label element if label prop is set"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "use-loader",
      "description": "Set to `true` or `false` to enable or disable spinner in the input"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"silent\"",
      "prop": "validate-mode",
      "description": "Valid values are 'silent' or 'eager'. See Form Validation for explanation and examples"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"blur\"",
      "prop": "validate-on",
      "description": "Valid values are 'blur', 'immediate' or 'form'. See Form Validation for explanation and examples"
    },
    {
      "type": [
        "String"
      ],
      "default": "null",
      "prop": "validation-state",
      "description": ""
    },
    {
      "type": [
        "String",
        "Boolean"
      ],
      "default": "empty string",
      "prop": "variant",
      "description": "Variant of the component styles"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "width",
      "description": "Width of the component in px"
    }
