{
  "name": "vInput",
  "description": "",
  "props": [
    {
      "type": [
        "String",
        "Number",
        "Array",
        "Boolean",
        "Number"
      ],
      "default": "empty string",
      "prop": "v-model",
      "description": "Input v-model"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"input\"",
      "globalDefault": true,
      "prop": "base",
      "description": "Base name used to style component"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true,
      "prop": "clearable",
      "description": "Adds 'X' button that resets model to default value"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "globalDefault": true,
      "prop": "close-button",
      "description": "Attributes of input v-close-button component (clear button)",
      "tags": "attributes"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "custom-clearable",
      "description": "Adds 'X' button that only emits @click:clear-button event. It does not modify model value"
    },
    {
      "type": [
        "Object"
      ],
      "default": {
        "class": "absolute"
      },
      "globalDefault": true,
      "prop": "form-text",
      "description": "Attributes of v-form-text component (validation messages)"
    },
    {
      "type": [
        "String",
        "Object"
      ],
      "default": "empty string",
      "prop": "icon",
      "description": "Adds icon to input element. Valid value is the same as in v-icon components"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true,
      "prop": "inline",
      "description": "Sets inputs display to inline"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "is-loading",
      "description": "If true displays spinner in the input. Spinner should be first enabled in `use-loading` prop"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "label",
      "description": "Label on top of the input. This label is for simple forms as position cannot be changed"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-close-button-wrapper",
      "description": "Style of the wrapper element of the clear button"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-icon",
      "description": "Style of the icon element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-input",
      "description": "Style of the main input element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-label",
      "description": "Style of the label element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "name",
      "description": "Name of the component"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "no-messages",
      "description": "If `true` validation messages are not displayed"
    },
    {
      "type": [
        "String"
      ],
      "default": "undefined",
      "prop": "role",
      "description": ""
    },
    {
      "type": [
        "Object"
      ],
      "default": [],
      "prop": "rules",
      "description": "Rules for validation. See Form Validation for examples of rules"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true,
      "prop": "single-line-message",
      "description": "Limits the number of displayed errors to one at time. Order of messages is the same as order of `rules` in rules prop"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "globalDefault": true,
      "prop": "spinner",
      "description": "Attributes of input v-spinner component (loader)"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "use-loader",
      "description": "Enable or disable spinner in the input"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"silent\"",
      "prop": "validate-mode",
      "description": "Valid values are 'silent' or 'eager'. See Form Validation for explanation and examples"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"blur\"",
      "prop": "validate-on",
      "description": "Valid values are 'blur', 'immediate' or 'form'. See Form Validation for explanation and examples"
    },
    {
      "type": [
        "String"
      ],
      "default": "null",
      "prop": "validation-state",
      "description": "Overrides state of input validity. Supported values are 'valid', 'invalid', `empty string` for default state or `null` for component controlled state"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "variant",
      "description": "Active variant of the element `mod-*` props"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "width",
      "description": "Width of the component in px"
    }
  ],
  "events": [
    {
      "event": "update:modelValue",
      "description": "Updates v-model of component"
    },
    {
      "event": "validation:status",
      "description": "Emmited after input validation. Event data contain `object` with validation result details"
    },
    {
      "event": "validation:messages",
      "description": "Emmited after input validation. Event data contains validation messages or errors"
    },
    {
      "event": "validation:state",
      "description": "Emmited after input validation. Event data contains validation state"
    },
    {
      "event": "click:clear-button",
      "description": "Emitted after clicking clear button (if `custom-clearable` prop is set)"
    },
    {
      "event": "click:icon",
      "description": "Emitted after clicking input icon"
    },
    {
      "event": "click:indicator",
      "description": "Emitted after clicking input indicator"
    }
  ],
  "slots": [
    {
      "slot": "label",
      "description": "Slot for label. Slot props: `label` (from label prop)"
    },
    {
      "slot": "icon",
      "description": "Slot for icon"
    },
    {
      "slot": "prepend",
      "description": "Slot for content prepending input"
    },
    {
      "slot": "append",
      "description": "Slot for content appending input"
    }
  ],
  "functions": [],
  "components": []
}