{
  "name": "vInput",
  "description": "",
  "props": [
    {
      "type": [
        "String",
        "Number",
        "Array",
        "Boolean",
        "Number"
      ],
      "default": "empty string",
      "prop": "v-model",
      "description": "Input v-model"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"input\"",
      "prop": "base",
      "description": "Base name of the component for styling"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "prop": "chevron",
      "description": "Attributes of input v-chevron component (indicator)"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": "undefined",
      "prop": "clearable",
      "description": "Adds 'X' button that sets model to empty string"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "prop": "close-button",
      "description": "Attributes of input v-close-button component (clear button)"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "custom-clearable",
      "description": "Adds 'X' button that only emits @click:clear-button event. It does not modify model value"
    },
    {
      "type": [
        "Object"
      ],
      "default": {
        "class": "absolute"
      },
      "prop": "form-text",
      "description": "Attributes of v-form-text component (validation messages)"
    },
    {
      "type": [
        "String",
        "Object"
      ],
      "default": "empty string",
      "prop": "icon",
