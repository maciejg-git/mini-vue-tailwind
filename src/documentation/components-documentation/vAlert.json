{
  "name": "vAlert",
  "description": "",
  "props": [
    {
      "type": [
        "Boolean"
      ],
      "default": "undefined",
      "prop": "v-model",
      "description": "Visiblity state of alert. Skip v-model to make alert static"
    },
    {
      "type": [
        "Number"
      ],
      "default": 0,
      "prop": "auto-dismiss-delay",
      "description": "Delay after which alert is closed"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"alert\"",
      "prop": "base",
      "description": "Base name used to style component"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "prop": "close-button",
      "description": "Attributes of v-close-button component. See documentation for valid values"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": true,
      "prop": "dismissable",
      "description": "If false close button is not rendered and alert cannot be dissmised"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "icon",
      "description": "Alert has few default icons for variants: 'success', 'warn', 'danger', 'info', 'valid', 'invalid'. Icon or default slot can be used for custom icons."
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "name",
      "description": "Name of the component"
    },
    {
      "type": [
        "String",
        "Array"
      ],
      "default": "empty string",
      "prop": "style-alert",
      "description": "Main alert element"
    },
    {
      "type": [
        "String",
        "Array"
      ],
      "default": "empty string",
      "prop": "style-icon",
      "description": "Default icon element (does not apply to icons in slot)"
    },
    {
      "type": [
        "String",
        "Boolean"
      ],
      "default": "empty string",
      "prop": "variant",
      "description": "Active variant of the elements style"
    }
  ],
  "events": [
    {
      "event": "update:modelValue",
      "description": "Updates v-model of component"
    }
  ],
  "slots": [
    {
      "slot": "icon",
      "description": "Slot for icon prepending message"
    },
    {
      "slot": "default",
      "description": "Slot for alert content"
    }
  ],
  "functions": [],
  "components": []
}