{
  "name": "vModal",
  "description": "",
  "props": [
    {
      "type": [
        "Boolean"
      ],
      "default": "undefined",
      "prop": "v-model",
      "description": "Set v-model to true to show modal or false to hide it. Model is optional if dropdown is activated from v-trigger component"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "globalDefault": true,
      "prop": "backdrop",
      "description": ""
    },
    {
      "type": [
        "String"
      ],
      "default": "\"modal\"",
      "globalDefault": true,
      "prop": "base",
      "description": "Base name used to style component"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "globalDefault": true,
      "prop": "close-button",
      "description": "Attributes of v-close-button component. See documentation for valid values"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"end\"",
      "globalDefault": true,
      "prop": "justify-buttons",
      "description": "Use this prop to place buttons in footer. Valid values are: 'start', 'end', 'center', 'between', 'around', 'evenly'"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-content",
      "description": "Style of content part of modal window"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-footer",
      "description": "Style of modal footer"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-header",
      "description": "Style of modal header"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-modal",
      "description": "Style of modal window"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "name",
      "description": "Name of the component"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "no-close-button",
      "description": "Disables 'X' button in modal header"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true,
      "prop": "no-footer",
      "description": "Do not render footer if `true`"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true,
      "prop": "no-header",
      "description": "If true modal dialog cannot be closed by clicking outside of it"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "no-primary-button",
      "description": "Disables primary (OK) button"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "no-secondary-button",
      "description": "Disables secondary (Cancel) button"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"top\"",
      "globalDefault": true,
      "prop": "position",
      "description": "Modal position. Valid values are: 'top', 'center' or 'bottom'"
    },
    {
      "type": [
        "Object"
      ],
      "default": {
        "styleButton": "primary"
      },
      "prop": "primary-button",
      "description": "Attributes of primary v-button component. See documentation for valid values"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "primary-button-close",
      "description": "If true clicking primary button emits @input:secondaryButtonClick event and closes modal. If false modal is not closed after emitting event"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"Accept\"",
      "globalDefault": true,
      "prop": "primary-button-label",
      "description": "Label of primary button"
    },
    {
      "type": [
        "Object"
      ],
      "default": {
        "styleButton": "secondary"
      },
      "prop": "secondary-button",
      "description": "Attributes of secondary v-button component. See documentation for valid values"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "secondary-button-close",
      "description": "If true clicking primary button emits @input:primaryButtonClick event and closes modal. If false modal is not closed after emitting event"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"Close\"",
      "globalDefault": true,
      "prop": "secondary-button-label",
      "description": "Label of secondary button"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"md\"",
      "globalDefault": true,
      "prop": "size",
      "description": "Modal size. Valid values are: 'sm', 'md', 'lg', 'xl' or 'fit' "
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "static-backdrop",
      "description": "If true modal dialog cannot be closed by clicking outside of it"
    },
    {
      "type": [
        "String"
      ],
      "default": "undefined",
      "prop": "title",
      "description": "Title displayed in modal header"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"fade-slide\"",
      "globalDefault": true,
      "prop": "transition",
      "description": "Sets animation effect when showing or hiding modal. Valid values are: 'fade', 'fade-slide', 'fade-scale' or `empty string` to disable animations. Animation speed can be set in `--modal-transition-duration` css variable."
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "variant",
      "description": "Active variant of the elements style"
    }
  ],
  "events": [
    {
      "event": "input:primary-button-click",
      "description": "Emmited after clicking primary button"
    },
    {
      "event": "input:secondary-button-click",
      "description": "Emmited after clicking secondary button"
    },
    {
      "event": "update:modelValue",
      "description": "Updates v-model of component"
    },
    {
      "event": "input:static-backdrop-click",
      "description": "Emmited after clicking backdrop if `static-backdrop` prop is `true`"
    }
  ],
  "slots": [
    {
      "slot": "modal",
      "description": "Swaps the entire default content of v-modal. Slot props: `close()`"
    },
    {
      "slot": "header",
      "description": "Modal header content"
    },
    {
      "slot": "default",
      "description": "Modal content"
    },
    {
      "slot": "footer",
      "description": "Modal footer content"
    }
  ],
  "functions": [],
  "components": []
}