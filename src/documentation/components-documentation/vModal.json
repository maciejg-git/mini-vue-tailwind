{
  "name": "vModal",
  "description": "",
  "props": [
    {
      "type": [
        "Boolean"
      ],
      "default": "undefined",
      "prop": "v-model",
      "description": "Set the v-model to `true` to show modal or `false` to hide it."
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "globalDefault": true,
      "prop": "backdrop",
      "description": "Attributes of the v-backdrop component",
      "tags": "attributes"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"modal\"",
      "globalDefault": true,
      "prop": "base",
      "description": "Base name used to style component"
    },
    {
      "type": [
        "Object"
      ],
      "default": {},
      "globalDefault": true,
      "prop": "close-button",
      "description": "Attributes of the v-close-button component. See documentation for valid values",
      "tags": "attributes"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"end\"",
      "globalDefault": true,
      "prop": "justify-buttons",
      "description": "Use this prop to place buttons in the footer. Valid values are: 'start', 'end', 'center', 'between', 'around', 'evenly'"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-content",
      "description": "Style of the content part of modal element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-footer",
      "description": "Style of the modal footer element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-header",
      "description": "Style of the modal header element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "globalDefault": true,
      "prop": "mod-modal",
      "description": "Style of the main modal element"
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "name",
      "description": "Name of the component"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "no-close-button",
      "description": "Disables the 'X' button in modal header"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true,
      "prop": "no-footer",
      "description": "Do not render the footer if `true`"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "globalDefault": true,
      "prop": "no-header",
      "description": "If true modal dialog cannot be closed by clicking outside of it"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "no-primary-button",
      "description": "Disables primary (OK) button"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "no-secondary-button",
      "description": "Disables secondary (Cancel) button"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"top\"",
      "globalDefault": true,
      "prop": "position",
      "description": "Position of the modal. Valid values are: 'top', 'center' or 'bottom'"
    },
    {
      "type": [
        "Object"
      ],
      "default": {
        "modButton": "variant:primary"
      },
      "prop": "primary-button",
      "description": "Attributes of the primary v-button component. See documentation for the valid values",
      "tags": "attributes"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "primary-button-close",
      "description": "If true clicking primary button emits @input:secondaryButtonClick event and closes modal. If false modal is not closed after emitting event"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"Accept\"",
      "globalDefault": true,
      "prop": "primary-button-label",
      "description": "Label of the primary button"
    },
    {
      "type": [
        "Object"
      ],
      "default": {
        "modButton": "variant:secondary"
      },
      "prop": "secondary-button",
      "description": "Attributes of the secondary v-button component. See documentation for the valid values",
      "tags": "attributes"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "secondary-button-close",
      "description": "If true clicking primary button emits @input:primaryButtonClick event and closes modal. If false modal is not closed after emitting event"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"Close\"",
      "globalDefault": true,
      "prop": "secondary-button-label",
      "description": "Label of the secondary button"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"md\"",
      "globalDefault": true,
      "prop": "size",
      "description": "Size of the modal on medium and larger screens. Valid values are: 'sm', 'md', 'lg', 'xl' or 'fit'"
    },
    {
      "type": [
        "Boolean"
      ],
      "default": false,
      "prop": "static-backdrop",
      "description": "If true modal dialog cannot be closed by clicking outside of it"
    },
    {
      "type": [
        "String"
      ],
      "default": "undefined",
      "prop": "title",
      "description": "Title displayed in the modals header"
    },
    {
      "type": [
        "String"
      ],
      "default": "\"fade-slide\"",
      "globalDefault": true,
      "prop": "transition",
      "description": "Sets animation effect when showing or hiding modal. Valid values are: 'fade', 'fade-slide', 'fade-scale' or `empty string` to disable animations. Animation speed can be set in `--modal-transition-duration` css variable."
    },
    {
      "type": [
        "String"
      ],
      "default": "empty string",
      "prop": "variant",
      "description": "Active variant of the element `mod-*` props"
    }
  ],
  "events": [
    {
      "event": "input:primary-button-click",
      "description": "Emmited after clicking primary button"
    },
    {
      "event": "input:secondary-button-click",
      "description": "Emmited after clicking secondary button"
    },
    {
      "event": "update:modelValue",
      "description": "Updates v-model of component"
    },
    {
      "event": "input:static-backdrop-click",
      "description": "Emmited after clicking backdrop if `static-backdrop` prop is `true`"
    }
  ],
  "slots": [
    {
      "slot": "modal",
      "description": "Swaps the entire default content of the v-modal. Slot props: `close()`"
    },
    {
      "slot": "header",
      "description": "Modal header content"
    },
    {
      "slot": "default",
      "description": "Modal content"
    },
    {
      "slot": "footer",
      "description": "Modal footer content"
    }
  ],
  "functions": [],
  "components": []
}